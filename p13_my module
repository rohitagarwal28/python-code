import datetime
import os
import sys
present=datetime.datetime.now()
name=input("enter your name")
name1=os.system("echo rohit | festival --tts")
if present.hour>=6 and present.hour<12:
	print("GOOD MORNING",name)
elif present.hour>=12 and present.hour<16:
	print("GOOD AFTERNOON",name)
elif present.hour>=16 and present.hour<19:
	print("GOOD EVENING",name)
elif present.hour>=29:
	print("GOOD NIGHT",name)
def add():
  input_string = input("Enter a list element separated by space ")
  list = input_string. split()
  print("Calculating sum of element of input list")
  sum = 0.
  for num in list:
  sum += int (num)
  print("Sum = ",sum)
def sort():
  sorted_data=input_string.sort()
  print(sorted_data)
def mod_info():
  print(help('modules'))
choice=int(input("enter your choice"))
if choice == 1:
  add()
elif choice == 2:
  sort()
elif choice == 3:
  mod_info()
else
  print("invalid choice")
